import io as sflio from ::_sfl::io;

/**
 * This is the IO class.
 */
class io {
    public const STDIN  = 0;
    public const STDOUT = 1;
    public const STDERR = 2;

    static public method print(... args) {
        return sflio.print(... args);
    }

    static public method printf(string format, ... args) {
        return ::_sfl::io.printf(format, ... args);
    }

}