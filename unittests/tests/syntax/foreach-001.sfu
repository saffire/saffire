import io;
foo = list[[1,2,3]];
foreach (foo as k)  {
    io.print(k,"\n");
}
====
1
2
3
@@@@
import io;
foo = hash[["a":1,"b":2,"c":3]];
foreach (foo as k,v)  {
    io.print(k,"=>",v,"\n");
}
====
